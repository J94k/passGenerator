{
  "manifest_version": 2,
  "name": "PassGen",
  "description": "Simple and fast password generator for your browser",
  "version": "1",
  "icons": {
    "32": "icons/key32.png",
    "48": "icons/key48.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "browser_style": true,
    "default_title": "PassGen",
    "default_popup": "index.html"
  }
}
